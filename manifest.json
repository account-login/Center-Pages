{
  "manifest_version": 2,
  "name": "Page Aligner",
  "description": "This extension let's you align pages in your browser",
  "version": "0.0.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "aligner.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
